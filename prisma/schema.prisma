generator client {
  provider = "prisma-client"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
}

model LogModel {
  id        Int           @id @default(autoincrement())
  message   String
  origin    String
  level     SeverityLevel
  createdAt DateTime      @default(now())
}

enum SeverityLevel {
  LOW
  MEDIUM
  HIGH
}
